VITE_CDN_BASE=https://zhangfc.cn/

# PDF libraries CDN base (default jsDelivr; replace with your OSS if needed)
VITE_PDF_LIB_BASE=https://zhangfc.cn/libs/html2pdf.bundle.min.js

# OSS signing configs (set real values in CI or local env)
OSS_REGION=oss-cn-hangzhou
OSS_BUCKET=zhangfc-resume
OSS_ENDPOINT=oss-cn-hangzhou.aliyuncs.com
# Use temporary or permanent AK/SK; if using STS add OSS_STS_TOKEN
OSS_AK=
OSS_SK=
OSS_STS_TOKEN=
# Sign URL expiry (seconds)
OSS_EXPIRES=2592000